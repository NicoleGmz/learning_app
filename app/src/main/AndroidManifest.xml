<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="nnar.learning_app">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.Learning_app">
        <activity android:name=".ui.mainmenu.MainMenuActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".ui.login.LoginActivity">
        </activity>
        <activity android:name=".ui.register.RegisterActivity">
        </activity>
        <activity android:name=".ui.contactListMenu.ContactListActivity">
        </activity>
        <activity android:name=".ui.contactDetail.ContactDetailActivity">
        </activity>
        <activity android:name=".ui.contactCreation.ContactCreationActivity">
        </activity>

    </application>

</manifest>